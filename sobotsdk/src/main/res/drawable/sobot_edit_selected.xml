<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
<!-- 评价机器人时 是否有以下问题。那些问题的selector -->
    <gradient
        android:angle="90"
        android:endColor="#2fb9c3"
        android:startColor="#2fb9c3" />
