        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginTop">18dp</item>
        <item name="android:background">@drawable/sobot_chat_msg_template_bg_left</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:minHeight">42dp</item>
    </style>

    <!--多轮会话聊天消息左侧气泡无 右 间距背景-->
    <style name="sobot_msg_left_content_ll_bg_no_right_style" parent="sobot_msg_left_content_ll_bg_style">
        <item name="android:layout_marginRight">0dp</item>
    </style>

    <!--聊天消息右侧气泡背景-->
    <style name="sobot_msg_right_content_ll_bg_no_padding_style">
        <item name="android:layout_marginLeft">60dp</item>
        <item name="android:layout_marginRight">12dp</item>
    </style>

    <!--语音消息右侧气泡背景  左侧无间距-->
    <style name="sobot_msg_audio_right_content_ll_bg_style" parent="sobot_msg_right_content_ll_bg_style">
        <item name="android:layout_marginLeft">0dp</item>
    </style>

    <!--文件消息右侧气泡背景 空心边框-->
