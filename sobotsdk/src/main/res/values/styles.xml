<resources>

    <style name="sobot_roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/sobot_rating_yellow</item>
        <item name="android:numStars">5</item>
    </style>

    <style name="sobot_small_roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/sobot_small_rating_yellow</item>
        <item name="android:numStars">5</item>
    </style>

    <style name="sobot_Dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="sobot_dialog_skill" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/sobot_popupwindow_in</item>
        <item name="android:windowExitAnimation">@anim/sobot_popupwindow_out</item>
    </style>

    <style name="sobot_dialog_dcrc" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>


    <style name="sobot_center_remind_note">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/sobot_toast_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/sobot_listview_remind_text_color</item>
        <item name="android:textSize">12sp</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>

    </style>

    <style name="sobot_center_remind_time" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/sobot_listview_remind_text_color</item>
        <item name="android:textSize">12sp</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_marginBottom">20dp</item>
    </style>

    <style name="sobot_chatting_panel_upload">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/sobot_color_bottom_upload_btn_color</item>
        <item name="android:textSize">12sp</item>
        <item name="android:visibility">visible</item>
    </style>

    <style name="sobot_chatting_nickname">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textColor">#AAAAAA</item>
        <item name="android:textSize">12sp</item>
        <item name="android:maxEms">13</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="sobot_chatting_imghead">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:background">@null</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:contentDescription">@null</item>
        <item name="android:visibility">visible</item>
    </style>

    <style name="sobot_clearHistoryDialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
    </style>
    <style name="sobot_FullScreenDialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
    </style>

    <style name="sobot_noAnimDialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="sobot_progress_circle">
        <item name="android:indeterminateDrawable">@drawable/sobot_progressbar_circle_loading</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="sobot_rb_style">
        <item name="android:textColor">@drawable/sobot_btn_text_color_selector</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:background">@drawable/sobot_evaluate_btn_selector</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">27dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!--邀请评价解决和未解决背景-->
    <style name="sobot_rb_evaluate_style">
        <item name="android:textColor">@drawable/sobot_btn_text_color_selector</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:background">@drawable/sobot_evaluate_yes_no_bg_2</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
    </style>

    <style name="sobot_cb_style">
        <item name="android:textColor">@drawable/sobot_btn_text_color_selector</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:background">@drawable/sobot_evaluate_btn_selector</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">27dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="sobot_dialog_Progress" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>


    <style name="sobot_custom_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- Dialog的windowFrame框为无 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否漂现在activity上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 是否半透明 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 去除黑色边框的关键设置项 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 屏幕背景是否变暗 -->
        <item name="android:backgroundDimAmount">0.3</item>
    </style>

    <style name="sobot_pickerview_dialogAnim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/sobot_pickerview_dialog_scale_in</item>
        <item name="android:windowExitAnimation">@anim/sobot_pickerview_dialog_scale_out</item>
    </style>

    <style name="sobot_tv_extend">
        <item name="android:drawablePadding">2dp</item>
        <item name="android:drawableRight">@drawable/sobot_icon_arrow_selector</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/sobot_bg_more_normal</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:text">@string/sobot_more</item>
        <item name="android:textColor">#0daeaf</item>
        <item name="android:textSize">14sp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="sobot_grid_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
<!--        <item name="android:layout_weight">1</item>-->
        <item name="android:background">@color/sobot_white</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:horizontalSpacing">10dp</item>
        <item name="android:verticalSpacing">10dp</item>
        <item name="android:numColumns">2</item>
        <item name="android:overScrollMode">never</item>
        <item name="android:scrollbarThumbVertical">@drawable/sobot_skill_group_scroll_img</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:stretchMode">columnWidth</item>
        <item name="android:listSelector">@android:color/transparent</item>
    </style>


    <!--聊天消息左侧气泡背景-->
    <style name="sobot_msg_left_content_ll_bg_style">
        <item name="android:layout_marginRight">60dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:background">@drawable/sobot_chat_msg_bg_left</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:minHeight">42dp</item>
    </style>
    <!--聊天消息左侧气泡背景_模版1-->
    <style name="sobot_msg_left_content_ll_bg_style_temp_1">
        <item name="android:layout_marginRight">60dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:background">@drawable/sobot_chat_msg_bg_left_temp_1</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingTop">13dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:minHeight">42dp</item>
    </style>

    <!--聊天消息右侧气泡背景-->
    <style name="sobot_msg_right_content_ll_bg_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/sobot_chat_msg_bg_right</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:minHeight">42dp</item>
    </style>


    <!--聊天消息气泡上下间距-->
    <style name="sobot_msg_top_bottom_style">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>


    <!--右侧发送消息  左侧空白区域  背景样式-->
    <style name="sobot_msg_left_empty_bg_style">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>


    <!--左侧发送消息  右侧空白区域  背景样式-->
    <style name="sobot_msg_right_empty_bg_style">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">72dp</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <!--发送消息 菊花 样式-->
    <style name="sobot_msg_send_loading_style">
        <item name="android:layout_width">18dp</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:layout_gravity">center_vertical|right</item>
    </style>

    <!--发送消息 发送结果 样式-->
    <style name="sobot_msg_send_status_style">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_gravity">center_vertical|right</item>
        <item name="android:background">@drawable/sobot_re_send_selector</item>
    </style>


    <!--聊天消息左侧气泡无内间距背景-->
    <style name="sobot_msg_left_content_ll_bg_no_padding_style">
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginRight">60dp</item>
    </style>

    <!--聊天消息左侧气泡无 右 间距背景-->
